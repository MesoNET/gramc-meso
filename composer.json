{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.8",
        "knplabs/knp-snappy-bundle": "^1.8",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "7.0.*",
        "symfony/config": "7.0.*",
        "symfony/console": "7.0.*",
        "symfony/dependency-injection": "7.0.*",
        "symfony/dotenv": "7.0.*",
        "symfony/expression-language": "7.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "7.0.*",
        "symfony/framework-bundle": "7.0.*",
        "symfony/http-client": "7.0.*",
        "symfony/intl": "7.0.*",
        "symfony/mailer": "7.0.*",
        "symfony/monolog-bundle": "^3.10.0",
        "symfony/process": "7.0.*",
        "symfony/property-access": "7.0.*",
        "symfony/property-info": "7.0.*",
        "symfony/routing": "7.0.*",
        "symfony/security-bundle": "7.0.*",
        "symfony/serializer": "7.0.*",
        "symfony/translation": "7.0.*",
        "symfony/twig-bundle": "7.0.*",
        "symfony/validator": "7.0.*",
        "symfony/web-link": "7.0.*",
        "symfony/yaml": "7.0.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "codeception/module-symfony": "^3.3",
        "friendsofphp/php-cs-fixer": "*",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/debug-bundle": "^7.0",
        "symfony/maker-bundle": "^1.52.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "^7.0",
        "symfony/var-dumper": "^7.0",
        "symfony/web-profiler-bundle": "^7.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": [ "src/", "src/" ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "db": [
            "sh reprise/reload-db ./reprise/gramc_reprise.sql",
            "php bin/console doctrine:migrations:diff",
            "php bin/console doctrine:migrations:migrate --allow-no-migration"
        ],
        "db:schema": [
            "sh reprise/reload-db ./reprise/schema.sql.dist",
            "php bin/console doctrine:migrations:diff",
            "php bin/console doctrine:migrations:migrate --allow-no-migration"
        ],
        "test:cs": [
            "vendor/bin/php-cs-fixer fix --dry-run"
        ],
        "fix:cs": [
            "vendor/bin/php-cs-fixer fix"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.0.*",
            "docker": false
        }
    }
}
